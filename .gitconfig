[user]
	name = shannon
	email = shannon.shu.227@gmail.com
[alias]
	lol = log --graph --decorate --pretty=oneline --abbrev-commit--all
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
[color]
	ui = true
[push]
	default = matching
[core]
	editor = vim
	excludesfile = /Users/shannon/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
